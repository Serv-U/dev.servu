<?php	$_items = $this->getValuesCollection()->getItems();	$attribute_code = $this->getData('attribute_code');	$_helper = $this->helper('sd_manager/manufacturerInfo');?><?php if (count($_items)):?><div class="footer-section">    <h5><?php echo __('Featured Brands') ?></h5>    <ul id="manufacturer-list" class="manufacturer-list-text">        <?php foreach ($_items as $item):?>            <li>        <?php            if($item->getUrlKey() != '') {                $urlKey = $item->getUrlKey();            } else {                $urlKey = $item->getIdentifier();            }    ?>            <a href="<?php echo $this->getUrl('/').$item->getAttributeCode().'/'.$urlKey ?>"            title="<?php echo $this->htmlEscape($item->getName())?>">            <?php echo $item->getValue(); ?></a>            </li>        <?php endforeach; ?>    </ul></div><?php endif;?>