<config>    <modules>        <ServU_ExportStatus>            <version>0.1.0</version> 	</ServU_ExportStatus>    </modules>    <global>        <resources>            <servu_exportstatus_setup>                <setup>                    <module>ServU_ExportStatus</module>                    <class>ServU_ExportStatus_Model_Mysql4_Setup</class>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </servu_exportstatus_setup>            <servu_exportstatus_write>                <connection>                    <use>core_write</use>                </connection>            </servu_exportstatus_write>            <servu_exportstatus_read>                <connection>                    <use>core_read</use>                </connection>            </servu_exportstatus_read>        </resources>        <models>            <exportstatus>                <class>Mage_Sales_Model</class>                <resourceModel>exportstatus_mysql4</resourceModel>            </exportstatus>            <exportstatus_mysql4>                <class>ServU_ExportStatus_Model_Mysql4</class>                <entities>                    <order><table>sales_flat_order</table></order>                </entities>            </exportstatus_mysql4>        </models>        <blocks>            <exportstatus>                <class>ServU_ExportStatus_Block</class>            </exportstatus>            <adminhtml>                <rewrite>                    <sales_order_grid>ServU_ExportStatus_Block_Adminhtml_Sales_Order_Grid</sales_order_grid>                </rewrite>            </adminhtml>        </blocks>    </global> </config>