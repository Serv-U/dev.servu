<config>    <modules>        <ServU_Shipping>            <version>0.3.5</version>            <depends>                <Mage_Shipping />            </depends>   	</ServU_Shipping>    </modules>    <frontend>        <routers>            <checkout>                <args>                     <modules>                         <ServU_Shipping before="Mage_Checkout">ServU_Shipping</ServU_Shipping>                     </modules>                 </args>             </checkout>        </routers>    </frontend>    <adminhtml>        <layout>            <updates>                <servu>                    <file>servu_shipping.xml</file>                 </servu>            </updates>        </layout>    </adminhtml>    <global>        <models>            <servu>                <class>ServU_Shipping_Model</class>            </servu>            <shipping>                <rewrite>                    <shipping>ServU_Shipping_Model_Shipping</shipping>                  </rewrite>            </shipping>            <usa>                <rewrite>                    <shipping_carrier_ups>ServU_Shipping_Model_Carrier_Ups</shipping_carrier_ups>                  </rewrite>            </usa>            <cataloginventory>                <rewrite>                    <stock_item>ServU_Shipping_Model_CatalogInventory_Stock_Item</stock_item>                </rewrite>            </cataloginventory>            <servu_shipping>                <class>ServU_Shipping_Model</class>                <resourceModel>servu_shipping_mysql4</resourceModel>            </servu_shipping>            <servu_shipping_mysql4>                <class>ServU_Shipping_Model_Mysql4</class>                <entities>                    <quote>                        <table>servu_shipping_misc_information_quote</table>                    </quote>                    <order>                        <table>servu_shipping_misc_information_order</table>                     </order>                </entities>            </servu_shipping_mysql4>        </models>        <resources>            <servu_shipping_setup>                <setup>                    <module>ServU_Shipping</module>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </servu_shipping_setup>            <servu_shipping_write>                <connection>                    <use>core_write</use>                </connection>            </servu_shipping_write>            <servu_shipping_read>                <connection>                    <use>core_read</use>                </connection>            </servu_shipping_read>        </resources>        <sales>            <shipping>                <carriers>                    <conway>                        <class>ServU_Shipping_Model_Carrier_Conway</class>                    </conway>                </carriers>            </shipping>            <quote>                <item>                    <product_attributes>                        <supplier/>                        <freight_class/>                        <ship_alone/>                        <free_shipping/>                        <always_truck/>                        <length/>                        <width/>                        <height/>                    </product_attributes>                </item>            </quote>        </sales>        <events>            <checkout_cart_save_before>                <observers>                    <save_before>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                        <method>saveCartBefore</method>                    </save_before>                </observers>            </checkout_cart_save_before>            <sales_quote_save_before>                <observers>                    <save_before>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                        <method>saveQuoteBefore</method>                     </save_before>                </observers>            </sales_quote_save_before>            <sales_quote_save_after>                 <observers>                    <save_after>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                        <method>saveQuoteAfter</method>                     </save_after>                </observers>            </sales_quote_save_after>            <sales_quote_load_after>                 <observers>                    <load_after>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                         <method>loadQuoteAfter</method>                      </load_after>                 </observers>            </sales_quote_load_after>            <sales_model_service_quote_submit_after>                 <observers>                    <sales_model_service_quote_submit_after>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                         <method>saveOrderAfter</method>                      </sales_model_service_quote_submit_after>                 </observers>            </sales_model_service_quote_submit_after>            <sales_order_load_after>                 <observers>                    <sales_order_load_after>                        <type>singleton</type>                        <class>ServU_Shipping_Model_Observer</class>                         <method>loadOrderAfter</method>                     </sales_order_load_after>                </observers>            </sales_order_load_after>        </events>        <blocks>            <adminhtml>                <rewrite>                    <sales_order_view_tab_info>ServU_Shipping_Block_Adminhtml_Sales_Order_View_Tab_Info</sales_order_view_tab_info>                  </rewrite>            </adminhtml>            <checkout>                <rewrite>                    <onepage_shipping_method_available>ServU_Shipping_Block_Checkout_Onepage_Shipping_Method_Available</onepage_shipping_method_available>                </rewrite>            </checkout>        </blocks>    </global>    <default>        <carriers>            <conway>                <active>0</active>		<sallowspecific>0</sallowspecific>		<conway_username backend_model="adminhtml/system_config_backend_encrypted"/>		<conway_password backend_model="adminhtml/system_config_backend_encrypted"/>		<conway_customer_number backend_model="adminhtml/system_config_backend_encrypted"/>                <ship_code>3</ship_code>                <freight_class>85</freight_class>                <model>servu/carrier_conway</model>		<name>Conway</name>		<title>Conway Freight</title>		<handling_fee>0</handling_fee>		<specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>            </conway>	</carriers>    </default></config>