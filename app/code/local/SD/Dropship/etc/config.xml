<?xml version="1.0" encoding="UTF-8"?><config>    <modules>        <SD_Dropship>            <version>0.0.1</version>        </SD_Dropship>    </modules>    <admin>        <routers>            <adminhtml>                <use>admin</use>                <args>                    <modules>                        <SD_Dropship before="Mage_Adminhtml">SD_Dropship_Adminhtml</SD_Dropship>                    </modules>                </args>            </adminhtml>        </routers>    </admin>    <global>        <models>            <sd_dropship>                <class>SD_Dropship_Model</class>                <resourceModel>sd_dropship_mysql4</resourceModel>            </sd_dropship>            <sd_dropship_mysql4>                <class>SD_Dropship_Model_Mysql4</class>                <entities>                    <supplier>                        <table>sd_dropship_supplier</table>                    </supplier>                </entities>            </sd_dropship_mysql4>        </models>        <resources>            <sd_dropship_setup>                <setup>                    <module>SD_Dropship</module>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </sd_dropship_setup>            <sd_dropship_write>                <connection>                    <use>core_write</use>                </connection>            </sd_dropship_write>            <sd_dropship_read>                <connection>                    <use>core_read</use>                </connection>            </sd_dropship_read>        </resources>        <blocks>            <sd_dropship><class>SD_Dropship_Block</class></sd_dropship>        </blocks>        <helpers>            <sd_dropship>                <class>SD_Dropship_Helper</class>            </sd_dropship>        </helpers>    </global>    </config>