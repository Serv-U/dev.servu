<?xml version="1.0"?><config>    <tabs>        <sd_tab translate="label">            <label>Six Demon</label>            <sort_order>500</sort_order>        </sd_tab>    </tabs>    <sections>        <acm translate="label">            <label>Abandoned Cart Mailer</label>            <tab>sd_tab</tab>            <sort_order>500</sort_order>            <show_in_default>1</show_in_default>            <show_in_website>1</show_in_website>            <show_in_store>1</show_in_store>            <groups>                <settings translate="label">                    <label>Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>10</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>                        <acm_enabled translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </acm_enabled>                        <recovered_enabled translate="label">                            <label>Send Emails After Cart Recovery</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>5</sort_order>                            <show_in_default>5</show_in_default>                            <show_in_website>5</show_in_website>                            <show_in_store>0</show_in_store>                        </recovered_enabled>                    </fields>                </settings>                <acm_first>                    <label>First Email Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>20</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>                        <email_template translate="label">                            <label>Abandoned Cart Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_template>                        <sender_identity translate="label">                            <label>Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </sender_identity>                        <elapsed_time translate="label">                            <label>Send After Specified Elapsed Time After Abandonment</label>                            <frontend_type>text</frontend_type>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Minutes</comment>                        </elapsed_time>                    </fields>                </acm_first>                <acm_second>                    <label>Second Email Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>30</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>                        <email_enabled translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_enabled>                        <email_template translate="label">                            <label>Abandoned Cart Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_template>                        <sender_identity translate="label">                            <label>Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </sender_identity>                        <elapsed_time translate="label">                            <label>Send After Specified Elapsed Time After First Email</label>                            <frontend_type>text</frontend_type>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Minutes</comment>                        </elapsed_time>                    </fields>                </acm_second>                <acm_third>                    <label>Third Email Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>40</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>                        <email_enabled translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_enabled>                        <email_template translate="label">                            <label>Abandoned Cart Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_template>                        <sender_identity translate="label">                            <label>Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </sender_identity>                        <elapsed_time translate="label">                            <label>Send After Specified Elapsed Time After Second Email</label>                            <frontend_type>text</frontend_type>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Minutes</comment>                        </elapsed_time>                        <cart_items translate="label">                            <label>Number of Items in Cart Required</label>                            <frontend_type>text</frontend_type>                            <sort_order>5</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>Leave blank to ignore this check</comment>                        </cart_items>                        <cart_amount translate="label">                            <label>Dollar Amount of Cart Required</label>                            <frontend_type>text</frontend_type>                            <sort_order>5</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>Leave blank to ignore this check</comment>                        </cart_amount>                    </fields>                </acm_third>            </groups>        </acm>    </sections></config>