<?xml version="1.0"?><config>    <tabs>        <sd_tab translate="label">            <label>Six Demon</label>            <sort_order>600</sort_order>        </sd_tab>    </tabs>    <sections>        <reviewreminder translate="label">            <label>Review Emailer</label>            <tab>sd_tab</tab>            <sort_order>500</sort_order>            <show_in_default>1</show_in_default>            <show_in_website>1</show_in_website>            <show_in_store>1</show_in_store>            <groups>                <settings translate="label">                    <label>Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>10</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>                        <reviewreminder_enabled translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </reviewreminder_enabled>                        <reviewreminder_promotion translate="label">                            <label>Shopping Cart Promotion ID</label>                            <frontend_type>text</frontend_type>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </reviewreminder_promotion>                        <promotion_email_template translate="label">                            <label>Promotion Email Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </promotion_email_template>                        <promotion_sender_identity translate="label">                            <label>Promotion Email Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </promotion_sender_identity>                    </fields>                </settings>                <reviewreminder_first>                    <label>First Email Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>20</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>                        <email_template translate="label">                            <label>Review Reminder Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_template>                        <sender_identity translate="label">                            <label>Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </sender_identity>                        <elapsed_time translate="label">                            <label>Elapsed Time After Completion</label>                            <frontend_type>text</frontend_type>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Days</comment>                        </elapsed_time>                        <default_time translate="label">                            <label>Default Elapsed Time</label>                            <frontend_type>text</frontend_type>                            <sort_order>5</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Days. Based on order date</comment>                        </default_time>                    </fields>                </reviewreminder_first>                <reviewreminder_second>                    <label>Second Email Settings</label>                    <frontend_type>text</frontend_type>                    <sort_order>30</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>0</show_in_store>                    <fields>                        <email_enabled translate="label">                            <label>Enable</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>1</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_enabled>                        <email_template translate="label">                            <label>Review Reminder Template</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_template</source_model>                            <sort_order>2</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </email_template>                        <sender_identity translate="label">                            <label>Sender</label>                            <frontend_type>select</frontend_type>                            <source_model>adminhtml/system_config_source_email_identity</source_model>                            <sort_order>3</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                        </sender_identity>                        <elapsed_time translate="label">                            <label>Elapsed Time After First Email</label>                            <frontend_type>text</frontend_type>                            <sort_order>4</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>0</show_in_store>                            <comment>In Days</comment>                        </elapsed_time>                    </fields>                </reviewreminder_second>            </groups>        </reviewreminder>    </sections></config>